{
    "name": "oefenweb/cakephp-social-media",
    "description": "A minimalistic SocialMedia Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp", "social media"
    ],
    "homepage": "http://github.com/Oefenweb/cakephp-social-media",
    "license": "MIT",
    "authors": [
        {
            "name": "Oefenweb.nl BV team",
            "homepage": "https://github.com/Oefenweb/cakephp-social-media/contributors"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "oefenweb/cakephp-codesniffer": "^3.0.0",
        "phpunit/phpunit": "^5.7|^6.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0"
    },
    "support": {
        "source": "https://github.com/Oefenweb/cakephp-social-media",
        "issues": "https://github.com/Oefenweb/cakephp-social-media/issues"
    },
    "autoload": {
        "psr-4": {
            "SocialMedia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SocialMedia\\Test\\": "tests",
            "SocialMedia\\Test\\TestApp\\": "tests/test_app/src",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
